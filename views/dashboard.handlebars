 {{>dashboard-title}}



{{#if posts.length}}
<section>
  <h2 id="posts-id">Your Posts: </h2>
  <ol>
    {{#each posts as |post|}}
    <li>
      {{> post-info post}}
      <a href="/dashboard/edit/{{post.id}}" class="edit-link">Edit post</a>
    </li>
    {{/each}}
  </ol>
</section>
{{/if}}

<div id="btn-new-post-holder">
   <button id="btn-new-post" class="btn-no-style"> + New Post</button>
</div>
 


<script src="./javascript/new-post.js"></script>
<script src="./javascript/logout.js"></script>
